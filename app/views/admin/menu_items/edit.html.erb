<%= render :partial => 'admin/shared/form_errors', :object => @menu_item %>

<%= simple_form_for @menu_item, :url => url_for(:controller => 'admin/menu_items', :action => 'update'), :method => 'put', :html => { :role => "form" } do |form| %>

  <%= render :partial => 'form', :object => form %>

  <%= submit_tag 'Submit Menu Item', :class => 'btn btn-primary' %>

<% end %>
